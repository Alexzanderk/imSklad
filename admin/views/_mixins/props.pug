mixin props(values)
    each val in values
        li.props__item
            +input('_id', val._id)(type='text' placeholder='name - имя поля для bd')
            +input('propsName', val.propsName)(type='text' placeholder='title - название свойства для таблицы')
            +select('propsUnit', val.propsUnit)(placeholder='ед.измерения' class='form-row--select')
            a(href="#").props__link.props__link--delete.deleteRowButton 
                i.far.fa-trash-alt.ico-delete